@using TodoApp.Objects
@model Homework
<td>
    @Html.DisplayFor(modelItem => Model.Title)
</td>
<td>
    @Html.DisplayFor(modelItem => Model.DateCreated)
</td>
<td>
    @Html.DisplayFor(modelItem => Model.DateModified)
</td>
<td>
    <div class="d-flex align-items-center">
        <a class="btn btn-outline-primary" asp-action="Edit" asp-controller="Home" asp-route-id="@Model.Id">Edit</a>
    </div>
    <div class="d-flex align-items-center">
        <button class="btn btn-outline-primary" onclick="DeleteToDo(event,this)" asp-action="Delete" asp-controller="Home" asp-route-id="@Model.Id">Delete</button>
    </div>
</td>